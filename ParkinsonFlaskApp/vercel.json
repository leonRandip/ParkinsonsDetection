{
  "builds": [{ "src": "app.py", "use": "@vercel/python" }],
  "functions": {
    "app.py": {
      "runtime": "python3.11",
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": ["templates/**", "static/**", "model/**", "Audio/**"]
    }
  },
  "routes": [{ "src": "/(.*)", "dest": "app.py" }]
}
